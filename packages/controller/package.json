{
  "name": "@abb/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "introspect": "apollo-codegen introspect-schema http://localhost:4000 --output schema.json",
    "generate": "apollo-codegen generate src/**/*.tsx --schema schema.json --target ts-modern",
    "gen-types": "npm run introspect && npm run generate"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-apollo": "^2.3.3",
    "graphql-tag": "^2.10.0",
    "graphql": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
